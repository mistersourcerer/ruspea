(def puts
  (fn [str]
    (.
      (:: Kernel) puts str)))

(def =
  (fn [lhs rhs]
    (cond
      (. lhs == rhs) true
      true false)))

(def !=
  (fn [lhs rhs]
    (cond
      (. lhs != rhs) true
      true false)))

(def >
  (fn [lhs rhs]
    (cond
      (. lhs > rhs) true
      true false)))

(def <
  (fn [lhs rhs]
    (cond
      (. lhs > rhs) true
      true false)))
